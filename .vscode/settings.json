{
  "code-runner.executorMap": {
    "python": "cd $dir && python3 $fileName < input.txt",
    "javascript": "cd $dir && node $fileName < input.txt",
    "rust": "cd $dir && rustc $fileName -o $fileNameWithoutExt && $dir/$fileNameWithoutExt < input.txt"
  },
  "code-runner.runInTerminal": true,
  "code-runner.saveFileBeforeRun": true,
  "code-runner.clearPreviousOutput": true,
  "code-runner.ignoreSelection": true
}
